  </servlet>
  <servlet-mapping>
    <servlet-name>PanValidatorServlet</servlet-name>
    <url-pattern>/PanValidatorServlet</url-pattern>
  </servlet-mapping>
  <filter>
    <display-name>SecondPancardValidator</display-name>
    <filter-name>SecondPancardValidator</filter-name>
